  procedures :
   -
     name : NewList
     procedure_id : 1640206100065625448
     type_code : 14
     code : |1-
      
      // Summary: Creates a new list
      //
      PROCEDURE NewList()
      
      sName is string
      
      //1: OK
      //2: Cancel
      SWITCH Input(<§?10A3B8D4002B14EE§>,sName )
      	// OK
      	CASE 1
      		IF sName <> "" THEN
      			// Check whether a list with the same name does not already exist
      			HReadSeek(List,Lastname,sName)
      			IF NOT HFound(List) THEN
      				// New list
      				HReset(List)
      				List.Lastname = sName
      				List.PositionLPR = LOOP_Lists..Occurrence + 1
      				HAdd(List)	
      				
      				// Open the list
      				OpenMobileWindow(WIN_List,List.ListID)
      			ELSE
      				Error("A list with the same name already exists.")
      				RETURN	
      			END
      			
      			
      		ELSE
      			Error("Enter a name for the list.")
      		END
      	// Cancel
      	CASE 2
      		
      END
     type : 458752
   -
     name : EditList
     procedure_id : 1640206100065690984
     type_code : 14
     code : |1-
      
      // Summary: Edits a list
      //
      PROCEDURE EditList()
      
      sName is string
      
      HReadSeek(List,ListID,LOOP_Lists[LOOP_Lists].ATT_ID)
      
      sName = List.Lastname
      
      //1: OK
      //2: Cancel
      SWITCH Input(<§?10A3B8D4002B14EE§>,sName )
      	// OK
      	CASE 1
      		IF sName <> "" THEN
      			// New list
      			List.Lastname = sName
      			HModify(List)
      			
      			LooperDisplay(LOOP_Lists)
      		ELSE
      			Error("Enter a name for the list.")
      		END
      	// Cancel
      	CASE 2
      		
      END
     type : 458752
   -
     name : SavePositions
     internal_properties : CAAAAAgAAADLrZLo6UeCjgzv7DgNibohXgtXMCc0BA3L8Xo4WFVJUUtegCANqt2R0plvZnTKw7zJPLjk5gqleQEQuyzBn2uHLsbnCtrXB0lrJdXDL3lNNTZ1y0zPb30hRE0QfL/L3/0sG/D1tlOZE8QWN2w=
     procedure_id : 1640206100065756520
     type_code : 14
     code : |1-
      
      // Summary: Saves the positions of the lists in the database according to the looper
      // 
      PROCEDURE SavePositions()
      
      FOR i = 1 _TO_ LOOP_Lists..Occurrence
      	HReadSeek(List,ListID,LOOP_Lists[i].ATT_ID)
      	IF HFound(List) THEN
      		List.PositionLPR = i
      		HModify(List)
      	END
      END
      
      LooperDisplay(LOOP_Lists)
     type : 458752
